style "handle"
{
	xthickness					= 0
	ythickness					= 0

	GtkPaned::handle-size				= 3

	engine "pixmap"
	{

		#Handles

		image
		{
		function				= HANDLE
		file					= "Images/null.png"
		orientation				= HORIZONTAL
		}

		image
		{
		function				= HANDLE
		file					= "Images/null.png"
		orientation				= VERTICAL
		}

		#Lines

		image
		{
		function				= VLINE
		file					= "Images/null.png"
		}

		image
